{
  "name": "nuxt-yup",
  "version": "2.0.0",
  "description": "yup implementation for nuxt",
  "repository": "DarlanPrado/nuxt-yup",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nuxt",
    "yup",
    "nuxt-module",
    "nuxt3"
  ],
  "homepage": "https://github.com/DarlanPrado/nuxt-yup#readme",
  "bugs": {
    "url": "https://github.com/DarlanPrado/nuxt-yup/issues"
  },
  "contributors": [
    {
      "name": "Darlan Jos√© Batista do Prado",
      "email": "darlanprado.contact@gmail.com",
      "url": "https://github.com/DarlanPrado"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@types/yup": "^0.32.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@nuxt/kit": "^4.0.2",
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.37.0",
    "nuxt": "^4.1.3",
    "typescript": "latest",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
